{
	"compilerOptions": {
		"outDir": "./built/",
		"sourceMap": true,
		"strict": true,
		"module": "ESNext",
		"moduleResolution": "node",
		"target": "ES2017",
		"skipLibCheck": true,
		"esModuleInterop": true,
		"experimentalDecorators": true,
		"noImplicitAny": false,
		"resolveJsonModule": true
	},
}